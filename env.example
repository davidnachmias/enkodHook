# AI Provider API Keys
# Copy this file to .env and add your actual API keys

# OpenAI API Key (for DALL-E)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Stability AI API Key (for Stable Diffusion)
VITE_STABILITY_API_KEY=your_stability_api_key_here

# Replicate API Key (for various models)
VITE_REPLICATE_API_KEY=your_replicate_api_key_here

# Optional: Default provider to use
VITE_DEFAULT_PROVIDER=openai

# Optional: Default model to use
VITE_DEFAULT_MODEL=dall-e-3

# Optional: Default image size
VITE_DEFAULT_SIZE=1024x1024 